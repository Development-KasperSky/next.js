    "webpack": "link:./node_modules/webpack5",
    "webpack-bundle-analyzer": "4.3.0",
    "worker-loader": "3.0.7"
  },
  "resolutions": {
    "browserslist": "4.18.1",
    "caniuse-lite": "1.0.30001283"
  },
  "engines": {
    "node": ">=12.22.0"
  },
  "turbo": {
    "pipeline": {
      "build-native": {
        "dependsOn": [
          "^build-native"
        ],
        "outputs": [
          "native/*.node"
        ]
      },
      "build-wasm": {
        "dependsOn": [
          "^build-wasm"
        ],
        "outputs": [
          "crates/wasm/pkg/*"
        ]
      }
    }
  }
}
