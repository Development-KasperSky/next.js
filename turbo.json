{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build-native": {
      "dependsOn": ["^build-native"],
      "outputs": ["native/*.node"]
    },
    "build-native-no-plugin": {
      "dependsOn": ["^build-native-no-plugin"],
      "outputs": ["native/*.node"]
    },
    "build-wasm": {
      "dependsOn": ["^build-wasm"],
      "outputs": ["crates/wasm/pkg/*"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "outputs": ["dist/**"]
    },
    "typescript": {}
  }
}
